{
    "name": "undash",
    "description": "An UNRAID Dashboard and API",
    "version": "0.0.0",
    "homepage": "",
    "main": "src",
    "keywords": [
        "feathers",
        "docker",
        "dashboard",
        "unraid"
    ],
    "license": "MIT",
    "repository": {},
    "author": {},
    "contributors": [],
    "bugs": {},
    "directories": {
        "lib": "src"
    },
    "engines": {
        "node": ">= 6.0.0",
        "yarn": ">= 0.18.0"
    },
    "scripts": {
        "test": "npm run xo && npm run  mocha",
        "xo": "./node_modules/.bin/xo src/ test/",
        "start": "./node_modules/.bin/babel-node src/",
        "mocha": "./node_modules/.bin/mocha test/ --recursive"
    },
    "dependencies": {
        "babel-cli": "^6.23.0",
        "babel-core": "^6.23.1",
        "babel-preset-es2015": "^6.22.0",
        "body-parser": "^1.17.0",
        "compression": "^1.6.2",
        "cors": "^2.8.1",
        "eslint": "^3.17.0",
        "feathers": "^2.1.1",
        "feathers-authentication": "pre",
        "feathers-authentication-jwt": "^0.3.1",
        "feathers-authentication-local": "^0.3.3",
        "feathers-configuration": "^0.4.1",
        "feathers-errors": "^2.6.0",
        "feathers-hooks": "^1.8.1",
        "feathers-hooks-common": "^2.0.3",
        "feathers-nedb": "^2.6.1",
        "feathers-rest": "^1.7.1",
        "feathers-socketio": "^1.5.2",
        "helmet": "^3.4.1",
        "mocha": "^3.2.0",
        "nedb": "^1.8.0",
        "request": "^2.80.0",
        "request-promise": "^4.1.1",
        "serve-favicon": "^2.4.1",
        "winston": "^2.3.1",
        "xo": "^0.17.1"
    },
    "xo": {
        "space": 4,
        "rules": {
            "space-before-function-paren": [
                "error",
                "never"
            ]
        },
        "parserOptions": {
            "ecmaVersion": 6
        },
        "envs": [
            "node",
            "es6",
            "mocha"
        ],
        "globals": [],
        "ignores": [
            "**/*.min.js"
        ]
    }
}
